<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tp.admin.dao.AdminEmployeeOperatingLogDao">
	<insert id="insertMaintionEmployeeOperatingLog" parameterType="com.tp.admin.data.entity.AdminEmployeeOperatingLog">
		INSERT INTO admin_employee_operating_log(
		maintion_id,
		employee_name,
		operate_name,
		title,
		intros,
		success
		)
		VALUES(
		#{maintionId},
		#{employeeName},
		#{operateName},
		#{title},
		#{intros},
		#{success})
	</insert>

	<insert id="insertMerchantEmployeeOperatingLog" parameterType="com.tp.admin.data.entity.AdminEmployeeOperatingLog">
		INSERT INTO admin_employee_operating_log(
		merchant_id,
		employee_name,
		operate_name,
		title,
		intros,
		success
		)
		VALUES(
		#{merchantId},
		#{employeeName},
		#{operateName},
		#{title},
		#{intros},
		#{success})
	</insert>

	<select id="list" parameterType="com.tp.admin.data.search.AdminEmployeeSearch" resultMap="AdminEmployeeOperatingLogMap">
		SELECT log.*
		FROM
		admin_employee_operating_log AS log
		<where>
		log.deleted=0
		<if test="opDesType != null">
			AND op_des_type = #{opDesType}
		</if>
		<if test="startTime != null and endTime != null">
			AND log.create_time BETWEEN #{startTime} AND #{endTime}
		</if>
		</where>
		ORDER BY log.create_time DESC
		<if test="pageSize != null and pageSize !=0 and offset != null">
			LIMIT #{offset},#{pageSize}
		</if>
	</select>

	<select id="cntOfEmployeeLog" parameterType="com.tp.admin.data.search.AdminEmployeeSearch" resultType="java.lang.Integer">
		SELECT COUNT(log.id)
		FROM
		admin_employee_operating_log AS log
		WHERE log.deleted=0
		<if test="opDesType != null">
			AND op_des_type = #{opDesType}
		</if>
		<if test="startTime != null and endTime != null">
			AND log.create_time BETWEEN #{startTime} AND #{endTime}
		</if>
	</select>

	<resultMap id="AdminEmployeeOperatingLogMap" type="com.tp.admin.data.entity.AdminEmployeeOperatingLog">
		<id column="id" property="id" jdbcType="INTEGER"/>
		<result column="create_time" property="createTime" jdbcType="TIMESTAMP"/>
		<result column="maintion_id" property="maintionId" jdbcType="VARCHAR"/>
		<result column="merchant_id" property="merchantId" jdbcType="VARCHAR"/>
		<result column="employee_name" property="employeeName" jdbcType="VARCHAR"/>
		<result column="operate_name" property="operateName" jdbcType="VARCHAR"/>
		<result column="title" property="title" jdbcType="VARCHAR"/>
		<result column="intros" property="intros" jdbcType="VARCHAR"/>
		<result column="msg" property="msg" jdbcType="VARCHAR"/>
		<result column="op_des_type" property="opDesType" jdbcType="INTEGER"/>
		<result column="deleted" property="deleted" jdbcType="BIT" />
		<result column="success" property="success" jdbcType="BIT" />

	</resultMap>
</mapper>