<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.tp.admin.dao.AdminAccountLoginLogDao">


    <insert id="insert" parameterType="com.tp.admin.data.entity.AdminAccountLoginLog" useGeneratedKeys="true" keyProperty="id"
            keyColumn="id">
        INSERT INTO `admin_account_login_log` (
			`username`,
			`msg`,
			`ip`,
			`sucess`
		) VALUES (
			#{username},
			#{msg},
			#{ip},
			#{sucess}
		)
    </insert>

</mapper>